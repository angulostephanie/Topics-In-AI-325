package hw5;

import java.util.Scanner;

public class Discount {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter in amount");
		String input = s.next();
		
		double inputD = Double.valueOf(input);
		if(inputD < 10.0) {
			System.out.println("Price too low");
		} else {
			double discount = inputD*.9;
			System.out.println("Discounted price: " + String.valueOf(discount));
		}

	}

}
